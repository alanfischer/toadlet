<!-- Phone specific targets go here -->
<project name="phones" basedir="../..">
	
	<!--
		These are just the phone names for easy targeting of the build process. 
		The phone.name var will determine what directory name your output gets
		The runtarget gives you the actual target being used
		Format is as follows:
		target name="manufacterer-name-carrier" depends="manufacterer-carrier-screensize-options_in_alphabetical_order)" description="phone name"
	-->

